          <label>State</label>
          <%= form.text_field :driving_liecense_state,value: address.state  %>
        </div>
        <div class="col-lg-4 col-md-6">
          <label>Driver's License Number</label>
          <%= form.text_field :driving_liecense_number, required: true, title:"Driving license number should be 7 characters"  %>
        </div>
        <div class="col-lg-4 col-md-6">
          <label>First Name</label>
          <%= form.text_field :driving_liecense_firstname  %>
        </div>
        <div class="col-lg-4 col-md-6">
          <label>Middle Name</label>
          <%= form.text_field :driving_liecense_middlename  %>
        </div>
        <div class="col-lg-4 col-md-6">
          <label>Last Name</label>
          <%= form.text_field :driving_liecense_lastname  %>
        </div>
      </div>
      
