<p id="notice"><%= notice %></p>

<h1>Car Features</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_features.each do |car_feature| %>
      <tr>
        <td><%= car_feature.name %></td>
        <td><%= link_to 'Show', car_feature %></td>
        <td><%= link_to 'Edit', edit_car_feature_path(car_feature) %></td>
        <td><%= link_to 'Destroy', car_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Feature', new_car_feature_path %>
