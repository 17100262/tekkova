<div class="container">
  <h3>My Listed Cars</h3>
  <table class="table">
    <thead>
      <tr>
        <th>Year</th>
        <th>Make</th>
        <th>Model</th>
        <th colspan=3></th>
      </tr>
    </thead>
    <tbody>
      <% current_user.cars.each do |car| %>
      <tr>
        <td><%= car.year %></td>
        <td> <%= car.make %></td>
        <td> <%= car.model %></td>
        <td><%= link_to 'Show', car_path(car) %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
      
    </tbody>
  </table>
</div>