car_details

<%= form_for @car, url: wizard_path, method: :put do |f| %>
  <%=  f.text_field :year  %>
  <%=  f.text_field :make  %>
  <%=  f.text_field :model  %>
  <%=  f.text_field :series  %>
  <%=  f.text_field :odometer  %>
  <%=  f.text_field :trans_mission  %>
  <%=  f.text_field :body_type  %>
  <%=  f.text_field :color  %>
  <%=  f.text_field :fuel_type  %>
  <%=  f.text_field :power  %>
  <%=  f.text_field :number_of_seats  %>
  <%=  f.text_field :drive_type  %>
  <%=  f.text_area :description  %>
  

  <%= f.submit "Change Password" %>
<% end %>